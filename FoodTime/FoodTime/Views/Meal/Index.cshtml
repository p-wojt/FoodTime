@model IEnumerable<MealModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Index Meal</h1>

<table class="table table-bordered table-striped" style="width: 100%">
    <thead>
    <tr>
        <th>
            Id
        </th>
        <th>
            Name
        </th>
    </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td>
                    @obj.Id
                </td>
                <td>
                    @obj.Name
                </td>
            </tr>
        }
    </tbody>
</table>